{
  "name": "@hennus/test",
  "version": "0.0.1-Test",
  "description": "Una npm para discord ",
  "main": "index.js",
  "author": "OscarDev <oscargamer162004@gmail.com>",
  "license": "Apache-2.0",
  "typings": "",
  "keywords": [
    "api",
    "discord",
    "bots",
    "typescript",
    "botdev"
  ],
  "scripts": {
    "lint": "deno lint --rules-exclude=ban-ts-comment,no-inferrable-types,no-explicit-any",
    "fmt": "deno fmt && deno fmt --no-semicolons",
    "build#Npm": "npm run build#clean && npm publish",
    "build#test": "npm run build#clean-old && tsc --declaration",
    "test": "node Tests",
    "build#clean-old": "rimraf -rf app && npm run build#clean-old-type",
    "build#clean-old-type": "rimraf -rf typescript",
    "build#clean": "npm run build#clean-old && tsup && tsc && npm run clean#build",
    "clean#build": "rimraf typescript/index.js"
  },
  "homepage": "https://github.com/Oscar-Dev0/Hennus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oscar-Dev0/Hennus.git"
  },
  "bugs": {
    "url": "https://github.com/Oscar-Dev0/Hennus/issues"
  },
  "devDependencies": {
    "@sapphire/snowflake": "latest",
    "@types/node": "latest",
    "deno": "latest",
    "typescript": "latest",
    "rimraf": "latest"
  },
  "dependencies": {
    "@discordjs/core": "latest",
    "@discordjs/formatters": "latest",
    "axios": "latest",
    "tsup": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public",
    "tag": "discord"
  }
}
